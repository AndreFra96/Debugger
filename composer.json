{
    "name": "andrefra96/debugger",
    "type": "library",
    "description": "Debugger for specific web app database",
    "keywords": [
        "AndreFra96",
        "Debugger"
    ],
    "homepage": "https://github.com/AndreFra96/Debugger",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Francesco Brioschi",
            "email": "andrea.brioschi@rcamilano.it",
            "homepage": "http://www.test.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "twbs/bootstrap": "4.5.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AndreFra96\\Debugger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndreFra96\\Debugger\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
